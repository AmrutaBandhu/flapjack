#!/usr/bin/env ruby

require 'rubygems'
require 'yajl'
require 'yajl/json_gem'

data = { "batches" =>
          [
            { "id" => 1, "checks" =>
              [
              "id"        => 1,
              "parent_id" => nil,
              "child_id"  => nil,
              "command"   => "exit 0"
              ]
            }
          ]
        }

puts data.to_json

exit 0
